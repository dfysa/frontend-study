/* 领养申请⻚⾯样式 */
.apply-form {
	padding: 40rpx;
	background-color: #FFFAF0;
 }
 input {
	width: 95%;
	padding: 20rpx;
	border: 2rpx solid #FFD700;
	border-radius: 10rpx;
	margin-bottom: 40rpx;
 }
 button {
	width: 100%;
	padding: 18rpx;
	background-color: #FFD700;
	color: white;
	border-radius: 10rpx;
	text-align: center;
 }
 